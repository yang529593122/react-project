.listWrapper
  background-color #fff
  margin-top 29px
  overflow hidden

.exportBox
  width 100%
  text-align right

.viewBox
  width 1200px
  margin 0 auto
  border-radius 5px
  box-shadow 3px 3px 5px 5px rgba(189,189,189,0.5)
  padding 0 10px
  .title
    text-align center
    padding 40px 0 30px
  .linkImg
    color #1890ff
    cursor pointer
    word-break break-all

.allImg
  width 100%
  height 100%
  background rgba(35, 36, 31,0.8)
  position fixed
  top 0
  left 0
  z-index 10
  display flex
  justify-content center
  align-items center
  img
    max-width 100%
    max-height 100%

.textareaBox
  word-break break-all
  white-space: pre-wrap;
  word-wrap: break-word;


.searchArea
  width 100%
  height 36px
  background #fff



